{
  "name": "pasteninja",
  "displayName": "pasteninja",
  "description": "Upload your code to pastebin like a ninja",
  "version": "0.0.2",
  "author": {
    "name": "Filip Hodun",
    "email": "filiphodun@gmail.com"
  },
  "publisher": "fhodun",
  "engines": {
    "vscode": "^1.60.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/fhodun/pasteninja.git"
  },
  "activationEvents": [
    "onCommand:pasteninja.upload"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "pasteninja.upload",
        "title": "PasteNinja: Upload"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "axios": "^0.21.4"
  }
}
